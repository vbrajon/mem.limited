{
  "manifest_version": 2,
  "name": "memory.limited",
  "description": "Search in your history, bookmarks content.",
  "version": "1.0",
  "permissions": ["tabs", "history", "bookmarks", "chrome://favicon/"],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'; img-src chrome://favicon data:;",
  "icons": {
    "128": "icon.png"
  }
}
